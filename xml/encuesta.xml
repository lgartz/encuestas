			<ask value="¿Que tan probable es que recomiende el helado de fresa?" />
			<range minimum="0" maximum="10" />
		</question>
		<question number="10" type="psu-t" required="true">
			<ask value="¿Cual es su marca de helado favorita?" />
		</question>
		<question number="11" type="psm-t" required="true">
